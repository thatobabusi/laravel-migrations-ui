{
    "name": "thatobabusi/laravel-migrations-ui",
    "description": "A web-based GUI for creating and running migrations in Laravel.",
    "keywords": [
        "laravel",
        "migrations"
    ],
    "homepage": "https://github.com/thatobabusi/laravel-migrations-ui",
    "authors": [
        {
            "name": "Thato Babusi",
            "email": "thatobabusi@yahoo.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "doctrine/dbal": "^2.0",
        "laravel/framework": ">=5.6"
    },
    "require-dev": {
        "orchestra/testbench": ">=3.6",
        "phpunit/phpunit": "^7.0|^8.0",
        "php-coveralls/php-coveralls": "^2.0",
        "nunomaduro/collision": "^3.0|^4.1",
        "mockery/mockery": "^1.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ThatoBabusi\\MigrationsUI\\": "classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MigrationsUITests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ThatoBabusi\\MigrationsUI\\MigrationsUIServiceProvider"
            ]
        }
    }
}
